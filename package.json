{
  "name": "vue-blockies",
  "description": "## How to use: 1. Just use `npm install --save vue-blockies` 2. Use the components in your file directly.",
  "version": "0.1.0",
  "private": false,
  "main": "dist/vue-blockies.common.js",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --target lib --name vue-blockies ./src/components/index.js",
    "lint": "vue-cli-service lint",
    "gh-pages": "vue-cli-service build"
  },
  "dependencies": {
    "ethereum-blockies": "^0.1.1",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.10",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
